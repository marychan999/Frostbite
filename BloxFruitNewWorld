local win = Flux:Window("Frostbite", "Premium Lua", Color3.fromRGB(110, 200, 170), Enum.KeyCode.B)
local tab = win:Tab("BloxFruits", "http://www.roblox.com/asset/?id=6023426915")
local combat = win:Tab("Combat", "http://www.roblox.com/asset/?id=6022668888")

tab:Label("Welcome to Frostbite Paid Script BloxFruits")

tab:Toggle("AutoFarm Level", "Automatically Farm Level", false , function(t)
    _G.autofarm = t 
    spawn(function()
            while wait() do 
                if _G.autofarm then 

                    autofarm() 
                end
            end
        end)
end)

tab:Toggle("AutoBuso", "Automatically BusoHaki.", false , function(t)
    _G.haki = t 
    spawn(function()
                    haki() 
        end)
end)

tab:Toggle("AntiAFK", "Won't Be Kicked For Afk", false , function(t)
	spawn(
		function()
			local vu = game:GetService("VirtualUser")
			game:GetService("Players").LocalPlayer.Idled:connect(function()
			   vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
			   wait(1)
			   vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
			end)
		end)
end)

tab:Dropdown("Select Tools", weapon, function(t)
    _G.weapon = t
    spawn(function()
        while wait() do
            if _G.weapon then
                pcall(function()
                    equip2()
                end)
                end
        end
end)
end)

combat:Label("You Can Use Combat to Kill Someone.")


combat:Dropdown("Select Tools", weapon, function(t)
    _G.weapon = t
    spawn(function()
        while wait() do
            if _G.weapon then
                pcall(function()
                    equip2()
                end)
                end
        end
end)
end)

combat:Dropdown("Select Player", nameplayer, function(t)
        _G.kill = t
end)



combat:Toggle("Kill Player", "You Can Select And Kill Player", false , function(t)
    spawn(
        function()
            while wait() do
                pcall(function()
                for i,v in pairs(game:GetService("Players"):GetPlayers()) do
                    if v.Name == _G.kill then
                        v.Character.HumanoidRootPart.Size = Vector3.new(60, 60, 60)
                        repeat wait()
                        equip2()
            game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
VirtualUser:CaptureController()
            VirtualUser:ClickButton1(Vector2.new(851, 158), game:GetService("Workspace").Camera.CFrame)-- ตีเอง
       game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0,20,10)
       until  v.Humanoid.Health == 0 
                        end
end
end)
end
end)
end)

