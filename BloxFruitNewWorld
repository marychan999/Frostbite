end

    function autofarm()
        if LocalPlayer.PlayerGui.Main.Quest.Visible == false then 
            
            LevelCkeck()
            print()
            LocalPlayer.Character.HumanoidRootPart.CFrame = _G.MeCFrame
            
            wait(1.5)
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest",_G.Namequest, _G.numberquest)

        elseif  game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true  then 
            game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = _G.MeCFrame1
            pcall(function()
                repeat wait()
for i , v in pairs(game:GetService("Workspace").Enemies:GetChildren ()) do 
       if v.Name ==   _G.mobs  then 
          v.HumanoidRootPart.Size = Vector3.new(40, 40, 40)
             equip2()
                game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
VirtualUser:CaptureController()
                VirtualUser:ClickButton1(Vector2.new(851, 158), game:GetService("Workspace").Camera.CFrame)-- ตีเอง
                setsimulationradius(1e+1599,1e+1599)
           v.HumanoidRootPart.CFrame = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,20,10) 
          
           v.HumanoidRootPart.CanCollide = false
           end
end
until LocalPlayer.PlayerGui.Main.Quest.Visible == false or _G.autofarm == false
end)
end
end

function Melee()

local args = {
    [1] = "AddPoint",
    [2] = "Melee",
    [3] = 1
}
game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
pcall (function()
end)
end
