end)

misc2:Button("2x EXP", "Use Candy to 2x EXP.", function()

local args = {
    [1] = "Candies",
    [2] = "Buy",
    [3] = 1,
    [4] = 1
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end)

misc2:Button("Stats Refunds", "Use Candy to Refunds Stats.", function()

local args = {
	[1] = "Candies",
	[2] = "Buy",
	[3] = 1,
	[4] = 2
}
	
	game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end)

misc2:Button("Random DevilFruit", "Use Candy to RandomDevilFruit.", function()

local args = {
    [1] = "Candies",
    [2] = "Buy",
    [3] = 4,
    [4] = 1
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end)
