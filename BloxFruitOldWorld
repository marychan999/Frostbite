				_G.MeCFrame1 = CFrame.new(5563.09717, 103.428391, 4084.29541, -0.224899054, 0.974382043, 1.50501728e-05, 1.50501728e-05, 1.8954277e-05, -1, -0.974382043, -0.224899143, -1.8954277e-05)

